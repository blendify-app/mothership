{
  "expo": {
    "name": "my-app",
    "scheme": "blendapp",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BlendIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/BlendIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blend.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BlendIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.blend.myapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "blendapp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/BlendIcon.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "dev-pb2l7ig8ecavkase.us.auth0.com"
        }
      ]
    ]
  }
}
